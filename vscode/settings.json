{
    "window.zoomLevel": 1,
    "git.autofetch": true,
    "[markdown]":  {
        "editor.quickSuggestions": true,
        "editor.tabCompletion": "on",
        "editor.snippetSuggestions": "top"
    },
    "workbench.colorTheme": "One Dark Pro",
    "workbench.iconTheme": "file-icons",
    "liveServer.settings.donotShowInfoMsg": true,
    "git.ignoreMissingGitWarning": true,
    "files.associations": {
        "*.cjson": "jsonc",
        "*.wxss": "css",
        ".lintstagedrc": "json",
        ".huskyrc": "json",
    },
    "emmet.includeLanguages": {
        "wxml": "html",
        "javascript": "javascriptreact",
        "typescript": "typescriptreact"
    },
    "minapp-vscode.disableAutoConfig": true,
    "editor.quickSuggestions": {
        "strings": true
    },
    "search.followSymlinks": false,
    "editor.multiCursorModifier": "ctrlCmd",
    "editor.formatOnPaste": true,
    "element-helper.version": "2.4",
    "gitlens.views.repositories.files.layout": "tree",
    "terminal.external.osxExec": "iTerm.app",
    "typescript.tsserver.log": "verbose",
    "[jsonc]": {
        "editor.defaultFormatter": "HookyQR.beautify"
    },
    "[javascript]": {
        "editor.defaultFormatter": "HookyQR.beautify"
    },
    "[html]": {
        "editor.defaultFormatter": "vscode.html-language-features"
    },
    "breadcrumbs.enabled": true,
    "typescript.updateImportsOnFileMove.enabled": "always",
    "todohighlight.isEnable": true,
    "[json]": {
        "editor.defaultFormatter": "HookyQR.beautify"
    },
    "code-runner.runInTerminal": true,
    "command-runner.commands": {
        "cdg": "cdg",
        "up": "up $dir",
        "legos dev": "legos build --watch",
        "legos build": "legos build",
        "legos release": "legos release",
        "openDir": "open .",
        "Open Terminal Here": "open -a iTerm $dir",
        "leetcode": "leetcode submit $fileName"
    },
    "code-runner.enableAppInsights": false,
    "code-runner.fileDirectoryAsCwd": true,
    "files.autoSave": "off",
    "javascript.format.enable": false,
    "terminal.integrated.shell.osx": "/bin/zsh",
    "workbench.editor.enablePreview": false,
    "update.mode": "manual",
    "diffEditor.ignoreTrimWhitespace": true,
    "workbench.startupEditor": "welcomePage",
    "gitlens.views.compare.files.layout": "list",
    "sync.gist": "896b4f0248eaab4e2eb487b5407c5c57",
    "javascript.updateImportsOnFileMove.enabled": "always",
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": true
    },
    "eslint.validate": [
        "javascript",
        "javascriptreact",
        "typescript",
        "typescriptreact"
    ],
}